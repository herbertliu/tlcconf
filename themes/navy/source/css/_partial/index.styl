    font-size: 24px

.intro-feature-desc
  text-align: left
  font-size: 12px
  line-height: 14px
  @media mq-normal
    line-height: 24px
    font-size: 14px
#intro-feature-background
  color: rgb(53, 119, 255)
  margin: 40px
  line-height: 20px
  font-size: 14px
  @media mq-normal
    font-size: 24px
    line-height: 40px
    margin: 80px

#intro-user-wrap
  margin: 0 auto;
  text-align: center;
  padding-bottom: 100px;
