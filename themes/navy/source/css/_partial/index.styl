.speaker-wrap 
  .tab-header
    background: url('https://qpic.url.cn/feeds_pic/Q3auHgzwzM7Nic6SB9f5fIibbdLXuhzhQsR12zibUqic0icR4fc82ffg1KA/') center center
    height: 400px
    .inner
      height: 400px
      position: relative
      h1  
        text-align: center
        font-size: 36px
        color: #fff
        margin-top: 100px
        letter-spacing: 8px
      .tab-scroller-wrap
        position: absolute
        bottom: 0
        overflow-x: scroll
        overflow-y: hidden
        -webkit-overflow-scrolling: touch
        width: 100%
        .tab-items 
          width: 756px
          @media mq-normal
            width: 1190px
          li
            cursor: pointer
            float: left
            font-size: 14px
            color: #fff
            height: 80px
            text-align: center
            line-height: 80px
            background: rgba(255,255,255, 0.2)
            margin-right: 1px
            width: 150px
            @media mq-normal
              width: 237px
              font-size: 20px
              

.schedule-wrap 
  h1
    display: flex
    align-items: center
    i
      background: url(https://qpic.url.cn/feeds_pic/Q3auHgzwzM7jCviaKDeWGYrK7hkNGArFV6icMib43GkmgejX0Hnj6JbwQ/) no-repeat
      display: inline-block
      width: 24px
