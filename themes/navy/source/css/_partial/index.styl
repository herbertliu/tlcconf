  .tab-header
    background: url('https://qpic.url.cn/feeds_pic/Q3auHgzwzM5Au48fl0sn9fvVIaDvpRFftZU3YP6rD4P4hIibaicLqr8A/') center center
    height: 260px
    .inner
      height: 260px
      position: relative
      .tab-items 
        position: absolute
        bottom: 0
        li
          cursor: pointer
          float: left
          font-size: 20px
          color: #fff
          width: 237px
          height: 80px
          text-align: center
          line-height: 80px
          background: rgba(255,255,255, 0.2)
          margin-right: 1px

  .tab-content 
    table
      margin: 40px 0
      border-collapse: collapse
      font-size: 16px
      td 
        border: 1px #e1e1e1 solid         
        height: 160px   
        width: 250px
        text-align: center
      .bold
        font-weight: bold
      .blue 
        color: #3379ff
      p
        margin: 0 25px 
        line-height: 22px
        &.author
          margin-top: 10px

.apply-wrap
  .header
