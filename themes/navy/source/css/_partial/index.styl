#banner
  color: rgba(255, 255, 255, 0.8)
  text-align: center
  background: url(https://qpic.url.cn/feeds_pic/ajNVdqHZLLDOfgiciaAv3icgkwg4ACEAwUd5KZElNbargT2gEGichJy7QA/) center center
  background-size: contain
  height: 606.5px
  @media mq-normal
    height: 924px
    background-image: none
    background-size: cover
    background-position: center center
  .wrapper
    height: 100%
  .inner
    height: 100%
    display: -ms-flexbox
    display: flex
    -ms-flex-pack: center
    justify-content: center
    -ms-flex-align: center
    align-items: center
    flex-direction: column
    padding-top: 310px
    @media mq-normal
      padding-top: 620px

#banner-bio
  font-size: 16px
  color: rgb(214, 214, 214)
  text-align: center
  letter-spacing: 8px
  margin: 0px 0px 70px
  @media mq-normal
    font-size: 32px;

#banner-buttons
  margin-bottom: 100px
  a
    height: 50px
    font-size: 16px
    margin-bottom: 20px
    color: rgb(255, 255, 255)
    background-color: transparent
    line-height: 50px
    text-transform: none
    position: relative
    display: inline-block
    cursor: pointer
    text-align: center
    padding: 0px 58px
    text-decoration: none
    transition: all 0.3s ease-out
    border-radius: 50px
  .buy-ticket
    text-align: center
    background: linear-gradient(90deg, rgb(51, 121, 255), rgb(133, 44, 255))
    width: 160px
    height: 50px
    font-size: 14px
    line-height: 50px
    padding: 0
    border-radius: 2px
    @media mq-normal
      padding: 0px 60px
      border-radius: 6px
      height: 70px
      line-height: 70px
      font-size: 24px
      width: auto
#banner-start
  text-align: center
  padding: 40px 0
  @media mq-normal
    padding: 60px 0
    font-size: 18px

#banner-start-command
  background: lighten(color-navy, 10%)
  font-family: font-mono
  display: inline-block
  padding: 15px 20px
  &:before
    content: "$"
    opacity: 0.5
    padding-right: 10px

#banner-start-link
  color: #fff
  background: color-link
  display: inline-block
  padding: 15px
  text-decoration: none
  transition: 0.2s
  &:hover
    background: color-link-hover

#banner-share
  display: none
  padding-bottom: 60px
  @media mq-normal
    display: block

#intro-feature-title
  background: url(https://qpic.url.cn/feeds_pic/ajNVdqHZLLAt1ecjaF8Nr9iadArhicd4nNUVo7k3PBwSESKneSL0wuvw/) no-repeat
  margin: 60px auto 16px
  width: 271.5px
  height: 33.5px
  background-size: contain
  @media mq-normal
    width: 543px
    height: 67px

#intro-feature-list
  display: flex
  flex-flow: column
  display: -webkit-box
  -webkit-box-pack: justify
  -webkit-box-align: center
  padding-top: 10px
  @media mq-normal
    flex-flow: row wrap
    padding-top: 20px

.intro-feature-wrap
  padding-top: 20px;
  font-size: 18px;
  width: 100%;
  padding: 0px 40px 20px 40px
  @media mq-normal
    -webkit-box-flex: 1
    font-size: 18px
    flex: 0 0 50%
    margin: 0px 42px
    padding: 0px 20px

.intro-feature
  position: relative
  text-align: center
  @media mq-normal
    text-align: center

.intro-feature-icon
  color: color-link
  font-size: 36px
  padding-bottom: 13px
  text-align: center
  @media mq-normal
    padding-bottom: 26px
  img
    width: 110px
    height: 81px

.intro-feature-title
  font-family: Lato, "Helvetica Neue", Helvetica, Arial, sans-serif
  font-size: 16px
  color: rgb(51, 51, 51)
  font-weight: bold
  margin-bottom: 16px
